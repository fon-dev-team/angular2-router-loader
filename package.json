  },
  "bugs": {
    "url": "https://github.com/brandonroberts/angular2-router-loader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonroberts/angular2-router-loader.git"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.2.0",
    "mocha": "^3.0.2",
    "pmock": "^0.2.3",
    "should": "^11.1.0"
  }
}
